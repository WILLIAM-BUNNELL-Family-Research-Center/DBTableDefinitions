/* GRAVES TABLE */

DROP TABLE BUNNELL.GG;

CREATE TABLE BUNNELL.GG
       (GRAVE   CHAR(5) NOT NULL,
        GSTCTY  CHAR(5),
        GTNCTY  CHAR(15),
        GNAME   CHAR(24),

        PRIMARY KEY(GRAVE));

DROP INDEX BUNNELL.GG_STCTY;

CREATE INDEX BUNNELL.GG_STCTY ON BUNNELL.GG (GSTCTY);
