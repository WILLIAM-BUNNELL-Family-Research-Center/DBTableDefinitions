/* CHURCH TABLE */

DROP TABLE BUNNELL.CC;

CREATE TABLE BUNNELL.CC
(       CHURCH  CHAR(5) NOT NULL,
        CSTCTY  CHAR(5),
        CTNCTY  CHAR(15),
        CNAME   CHAR(24),

        PRIMARY KEY(CHURCH));

DROP INDEX BUNNELL.CC_STCTY;

CREATE INDEX BUNNELL.CC_STCTY ON BUNNELL.CC (CSTCTY);
