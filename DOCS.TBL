/* DOCUMENTS TABLE */

DROP TABLE GENSYS.DOCS;

CREATE TABLE GENSYS.DOCS
(
SCHEMA             VARCHAR(8) NOT NULL,
MEMBF              CHAR(6)    NOT NULL,
DOC_NO             INTEGER    NOT NULL,
DOC_DATE           CHAR(10),
TITLE              VARCHAR(25),
DESCRIPTION        VARCHAR(120),
TYPE               CHAR(3),
DOCUMENT           BLOB(1000K),

PRIMARY KEY (SCHEMA, MEMBF, DOC_NO));